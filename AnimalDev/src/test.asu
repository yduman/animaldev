  update digit to "0"
  text "arrayHeader" "array" (20, 80) color (0, 0, 0) depth 1 font SansSerif size 12 bold
  array "intArray" (20, 100)  color (0, 0, 0) fillColor (255, 255, 255) elementColor (0, 0, 0) elemHighlight (255, 0, 0) cellHighlight (255, 255, 0) horizontal length 10 "9" "8" "47" "6" "5" "4" "13" "12" "1" "0"  depth 1 font SansSerif size 12
  codegroup "sourceCode" at (40, 140) color (0, 0, 0) highlightColor (255, 0, 0) contextColor (0, 0, 255) font SansSerif size 12 bold depth 1
  addCodeLine "public int[] sort(int[] array, int radix) {" to "sourceCode"
  addCodeLine "int digitCount = getDigitCount(array);" to "sourceCode" indentation 1
  addCodeLine "int divisor = (int) Math.pow(10, digitCount);" to "sourceCode" indentation 1
  addCodeLine "americanFlagSort(array, 0, array.length, divisor, radix);" to "sourceCode" indentation 1
  addCodeLine "return array;" to "sourceCode" indentation 1
  addCodeLine "}" to "sourceCode"
  addCodeLine "private void americanFlagSort(int[] array, int start, int length, int divisor, int radix) {" to "sourceCode"
  addCodeLine "int[] counts = new int[radix];" to "sourceCode" indentation 1
  addCodeLine "int[] offsets = new int[radix];" to "sourceCode" indentation 1
  addCodeLine "for (int i = start; i < length; i++) {" to "sourceCode" indentation 1
  addCodeLine "int digit = getDigit(array[i], divisor, radix);" to "sourceCode" indentation 2
  addCodeLine "counts[digit]++;" to "sourceCode" indentation 2
  addCodeLine "}" to "sourceCode" indentation 1
  addCodeLine "offsets[0] = start;" to "sourceCode" indentation 1
  addCodeLine "for (int i = 1; i < radix; i++) {" to "sourceCode" indentation 1
  addCodeLine "offsets[i] = counts[i - 1] + offsets[i - 1];" to "sourceCode" indentation 2
  addCodeLine "}" to "sourceCode" indentation 1
  addCodeLine "for (int i = 0; i < radix; i++) {" to "sourceCode" indentation 1
  addCodeLine "while (counts[i] > 0) {" to "sourceCode" indentation 2
  addCodeLine "int origin = offsets[i];" to "sourceCode" indentation 3
  addCodeLine "int source = origin;" to "sourceCode" indentation 3
  addCodeLine "int num = array[source];" to "sourceCode" indentation 3
  addCodeLine "array[source] = -1;" to "sourceCode" indentation 3
  addCodeLine "do {" to "sourceCode" indentation 3
  addCodeLine "int digit = getDigit(num, divisor, radix);" to "sourceCode" indentation 4
  addCodeLine "int destination = offsets[digit]++;" to "sourceCode" indentation 4
  addCodeLine "counts[digit]--;" to "sourceCode" indentation 4
  addCodeLine "int tmp = array[destination];" to "sourceCode" indentation 4
  addCodeLine "array[destination] = num;" to "sourceCode" indentation 4
  addCodeLine "num = tmp;" to "sourceCode" indentation 4
  addCodeLine "source = destination;" to "sourceCode" indentation 4
  addCodeLine "} while (from != origin);" to "sourceCode" indentation 3
  addCodeLine "}" to "sourceCode" indentation 2
  addCodeLine "}" to "sourceCode" indentation 1
  addCodeLine "if (divisor > 1) {" to "sourceCode" indentation 1
  addCodeLine "for (int i = 0; i < radix; i++) {" to "sourceCode" indentation 2
  addCodeLine "int begin = (i > 0) ? offsets[i - 1] : start;" to "sourceCode" indentation 3
  addCodeLine "int end = offsets[i];" to "sourceCode" indentation 3
  addCodeLine "if (end - begin > 1) {" to "sourceCode" indentation 3
  addCodeLine "americanFlagSort(array, begin, end, divisor / 10, radix);" to "sourceCode" indentation 4
  addCodeLine "}" to "sourceCode" indentation 3
  addCodeLine "}" to "sourceCode" indentation 2
  addCodeLine "}" to "sourceCode" indentation 1
  addCodeLine "}" to "sourceCode"
  hide "introOutroLines" "Text1" "Text2" "Text3" "Text4" "Text5" "Text6" "Text7" "Text8" "Text9" "Text10" "Text11" "Text12" "Text13" "Text14" "Text15" 
